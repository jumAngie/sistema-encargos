<script src="https://code.jquery.com/jquery-3.5.1.js"></script>
<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

<div class="modal fade" id="Fabricas" tabindex="-1" role="dialog" aria-labelledby="Crear-Fabricas" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="crear-fabricas">Fabricas</h5>
            </div>
            <div class="modal-body">
                <label>Ingrese el nombre de la fábrica</label>
                <input type="text" name="txtFabrica" id="txtFabrica" class="form-control" />
                <label>Ingrese el teléfono de la fábrica</label>
                <input type="text" name="txtTelefono" id="txtTelefono" class="form-control" />
            </div>
            <div class="modal-footer">
                <button type="button" id="btnCancelar" class="btn btn-secondary" data-dismiss="modal">Cancelar</button>
                <a id="btn-insertar" onclick="Insertar()" class="btn btn-danger">Insertar</a>
            </div>
        </div>
    </div>
</div>

<script src="~/Scripts/CrearFabrica.js"></script>
